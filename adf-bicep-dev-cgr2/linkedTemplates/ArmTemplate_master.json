{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-bicep-dev-cgr2"
		},
		"ls_azsql_db1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "sql-bicep-dev-cgr2.database.windows.net"
		},
		"ls_azsql_db1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db-dev-bccga-1"
		},
		"pec-sql-db1_properties_privateLinkResourceId": {
			"type": "string",
			"defaultValue": "/subscriptions/3be2ce56-4a5f-4034-88d7-2953d1819ed3/resourceGroups/bicep-cga-dev-uks-rg-01/providers/Microsoft.Sql/servers/sql-bicep-dev-cgr2"
		},
		"pec-sql-db1_properties_groupId": {
			"type": "string",
			"defaultValue": "sqlServer"
		},
		"default_properties_whichEnv_value": {
			"type": "string",
			"defaultValue": "dev"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-bicep-dev-cgr2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"default_properties_whichEnv_value": {
						"value": "[parameters('default_properties_whichEnv_value')]"
					},
					"pec-sql-db1_properties_privateLinkResourceId": {
						"value": "[parameters('pec-sql-db1_properties_privateLinkResourceId')]"
					},
					"pec-sql-db1_properties_groupId": {
						"value": "[parameters('pec-sql-db1_properties_groupId')]"
					},
					"ls_azsql_db1_properties_typeProperties_server": {
						"value": "[parameters('ls_azsql_db1_properties_typeProperties_server')]"
					},
					"ls_azsql_db1_properties_typeProperties_database": {
						"value": "[parameters('ls_azsql_db1_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}